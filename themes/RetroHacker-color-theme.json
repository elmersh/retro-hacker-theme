{
  "name": "Retro Hacker",
  "type": "dark",
  "colors": {
      // Base colors
      "foreground": "#39FF14",
      "focusBorder": "#00FF00",
      "selection.background": "#39FF1440",
      "descriptionForeground": "#00FF00",
      "errorForeground": "#FF0000",
      "widget.shadow": "#00FF0020",
      
      // Editor
      "editor.background": "#0A1F0A",
      "editor.foreground": "#E0FFE0",
      "editor.lineHighlightBackground": "#1A3F1A",
      "editor.lineHighlightBorder": "#39FF1440",
      "editor.selectionBackground": "#2A6F2A",
      "editor.wordHighlightBackground": "#39FF1430",
      "editor.wordHighlightStrongBackground": "#39FF1450",
      "editor.findMatchBackground": "#39FF1440",
      "editor.findMatchHighlightBackground": "#39FF1420",
      "editor.findRangeHighlightBackground": "#39FF1420",
      "editor.hoverHighlightBackground": "#39FF1420",
      "editor.selectionHighlightBackground": "#39FF1430",
      "editorCursor.foreground": "#39FF14",
      "editorCursor.background": "#000000",
      "editorLineNumber.foreground": "#2d7421",
      "editorLineNumber.activeForeground": "#00FF00",
      "editorBracketMatch.background": "#39FF1440",
      "editorBracketMatch.border": "#39FF14",
      "editorGutter.modifiedBackground": "#FFD700",
      "editorGutter.addedBackground": "#39FF14",
      "editorGutter.deletedBackground": "#FF4444",
      "editorRuler.foreground": "#39FF1440",
      "editorCodeLens.foreground": "#39FF1480",
      "editorError.foreground": "#FF0000",
      "editorError.border": "#FF000080",
      "editorWarning.foreground": "#FFD700",
      "editorWarning.border": "#FFD70080",
      "editorInfo.foreground": "#39FF14",
      "editorInfo.border": "#39FF1480",
      "editorHint.foreground": "#39FF1490",
      "editorHint.border": "#39FF1440",
      
      // Sidebar and explorer
      "sideBar.background": "#172117",
      "sideBar.foreground": "#1c9207",
      "sideBar.border": "#39FF14",
      "sideBarTitle.foreground": "#39FF14",
      "sideBarSectionHeader.background": "#133713",
      "sideBarSectionHeader.foreground": "#39FF14",
      "sideBarSectionHeader.border": "#39FF14",
      
      // Lists and trees
      "list.activeSelectionBackground": "#39FF1440",
      "list.activeSelectionForeground": "#FFFFFF",
      "list.hoverBackground": "#39FF1420",
      "list.hoverForeground": "#FFFFFF",
      "list.inactiveSelectionBackground": "#39FF1430",
      "list.inactiveSelectionForeground": "#E0FFE0",
      "list.dropBackground": "#39FF1440",
      "list.highlightForeground": "#39FF14",
      "list.errorForeground": "#FF0000",
      "list.warningForeground": "#FFD700",
      
      // Activity bar
      "activityBar.background": "#0A1F0A",
      "activityBar.foreground": "#39FF14",
      "activityBar.border": "#39FF14",
      "activityBar.activeBorder": "#39FF14",
      "activityBar.activeBackground": "#39FF1420",
      "activityBarBadge.background": "#39FF14",
      "activityBarBadge.foreground": "#000000",
      
      // Title and tabs
      "titleBar.activeBackground": "#0A1F0A",
      "titleBar.activeForeground": "#39FF14",
      "titleBar.inactiveBackground": "#0A1F0A",
      "titleBar.inactiveForeground": "#39FF1480",
      "titleBar.border": "#39FF14",
      "tab.activeBackground": "#133713",
      "tab.activeForeground": "#39FF14",
      "tab.inactiveBackground": "#0A1F0A",
      "tab.inactiveForeground": "#39FF1480",
      "tab.border": "#39FF14",
      "tab.activeBorder": "#39FF14",
      "tab.activeModifiedBorder": "#FFD700",
      "tab.unfocusedActiveBorder": "#39FF1460",
      "tab.hoverBackground": "#39FF1420",
      "tab.hoverBorder": "#39FF14",
      
      // Status bar
      "statusBar.background": "#0A1F0A",
      "statusBar.foreground": "#39FF14",
      "statusBar.border": "#39FF14",
      "statusBar.debuggingBackground": "#133713",
      "statusBar.debuggingForeground": "#39FF14",
      "statusBar.debuggingBorder": "#39FF14",
      "statusBar.noFolderBackground": "#0A1F0A",
      "statusBar.noFolderForeground": "#39FF14",
      "statusBar.noFolderBorder": "#39FF14",
      "statusBarItem.hoverBackground": "#39FF1440",
      "statusBarItem.remoteBackground": "#39FF14",
      "statusBarItem.remoteForeground": "#000000",
      
      // Terminal
      "terminal.background": "#0A1F0A",
      "terminal.foreground": "#39FF14",
      "terminal.selectionBackground": "#39FF1440",
      "terminalCursor.foreground": "#39FF14",
      "terminalCursor.background": "#000000",
      "terminal.border": "#39FF14",
      "terminal.ansiBlack": "#000000",
      "terminal.ansiBlue": "#87CEEB",
      "terminal.ansiBrightBlue": "#ADD8E6",
      "terminal.ansiGreen": "#39FF14",
      "terminal.ansiBrightGreen": "#00FF00",
      "terminal.ansiMagenta": "#DDA0DD",
      "terminal.ansiBrightMagenta": "#EE82EE",
      "terminal.ansiRed": "#FF0000",
      "terminal.ansiBrightRed": "#FF4444",
      "terminal.ansiYellow": "#FFD700",
      "terminal.ansiBrightYellow": "#FFFF00",
      "terminal.ansiWhite": "#E0FFE0",
      "terminal.ansiBrightWhite": "#FFFFFF",
      "terminal.ansiBrightCyan": "#00FFFF",
      "terminal.ansiCyan": "#00CED1",
      
      // Input controls
      "input.background": "#0A1F0A",
      "input.foreground": "#39FF14",
      "input.border": "#39FF14",
      "input.placeholderForeground": "#39FF1480",
      "inputOption.activeBackground": "#39FF1440",
      "inputOption.activeBorder": "#39FF14",
      "inputValidation.errorBackground": "#FF000020",
      "inputValidation.errorBorder": "#FF0000",
      "inputValidation.warningBackground": "#FFD70020",
      "inputValidation.warningBorder": "#FFD700",
      
      // Scrollbar
      "scrollbarSlider.background": "#39FF1440",
      "scrollbarSlider.hoverBackground": "#39FF1460",
      "scrollbarSlider.activeBackground": "#39FF1480",
      "scrollbar.shadow": "#00FF0020",
      
      // Breadcrumbs
      "breadcrumb.foreground": "#39FF1490",
      "breadcrumb.focusForeground": "#39FF14",
      "breadcrumb.activeSelectionForeground": "#FFFFFF",
      "breadcrumbPicker.background": "#0A1F0A",
      
      // Settings
      "settings.headerForeground": "#39FF14",
      "settings.modifiedItemIndicator": "#FFD700",
      "settings.checkboxBackground": "#0A1F0A",
      "settings.checkboxForeground": "#39FF14",
      "settings.checkboxBorder": "#39FF14",
      
      // Debug
      "debugToolBar.background": "#0A1F0A",
      "debugToolBar.border": "#39FF14",
      "debugIcon.breakpointForeground": "#FF0000",
      "debugIcon.startForeground": "#39FF14",
      
      // Git decorations
      "gitDecoration.modifiedResourceForeground": "#FFD700",
      "gitDecoration.deletedResourceForeground": "#FF0000",
      "gitDecoration.untrackedResourceForeground": "#39FF14",
      "gitDecoration.ignoredResourceForeground": "#39FF1440",
      "gitDecoration.conflictingResourceForeground": "#FFA500",
      "gitDecoration.submoduleResourceForeground": "#39FF1490"
  },
  "tokenColors": [
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#39FF1490",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String and Templates",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated",
                "string.regexp",
                "string.tag",
                "string.value"
            ],
            "settings": {
                "foreground": "#FFA07A"
            }
        },
        {
            "name": "JavaScript Template Expression",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color",
                "constant.other",
                "constant.character"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars",
                "meta.function-call",
                "meta.method-call",
                "meta.function.parameters"
            ],
            "settings": {
                "foreground": "#87CEEB",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof"
            ],
            "settings": {
                "foreground": "#39FF14",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types and Classes",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "storage.type.built-in",
                "meta.return-type",
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class",
                "entity.name.class",
                "source.cs storage.type"
            ],
            "settings": {
                "foreground": "#DDA0DD",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "support.variable",
                "variable.other",
                "variable.language",
                "variable.parameter",
                "meta.definition.variable.name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#98FB98"
            }
        },
        {
            "name": "Language Variables",
            "scope": [
                "variable.language.this",
                "variable.language.super",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#39FF14",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Punctuation and Brackets",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.brackets",
                "meta.parentheses",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.css",
                "entity.other.attribute-name",
                "source.css entity.other.attribute-name",
                "source.css.less entity.other.attribute-name.id",
                "source.scss entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#39FF14",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Properties and Values",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass",
                "support.type.vendored.property-name",
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media"
            ],
            "settings": {
                "foreground": "#98FB98"
            }
        },
        {
            "name": "Markdown",
            "scope": [
                "markup.heading",
                "markup.bold",
                "markup.italic",
                "markup.inserted",
                "markup.deleted",
                "markup.changed",
                "markup.underline"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Git and Version Control",
            "scope": [
                "support.function.git-rebase",
                "constant.sha.git-rebase",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        }
    ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
      "namespace": "#DDA0DD",
      "type": "#DDA0DD",
      "class": "#DDA0DD",
      "enum": "#DDA0DD",
      "interface": "#DDA0DD",
      "struct": "#DDA0DD",
      "typeParameter": "#DDA0DD",
      "parameter": "#98FB98",
      "variable": "#98FB98",
      "property": "#98FB98",
      "enumMember": "#FFD700",
      "event": "#87CEEB",
      "function": "#87CEEB",
      "method": "#87CEEB",
      "macro": "#39FF14",
      "keyword": "#39FF14",
      "modifier": "#39FF14",
      "comment": "#39FF1490",
      "string": "#FFA07A",
      "number": "#FFD700",
      "regexp": "#FFA07A",
      "operator": "#39FF14",
      "decorator": "#FFD700"
  }
}