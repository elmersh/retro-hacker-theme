      "terminal.ansiBrightBlack": "#3a2b1a",
      "terminal.ansiBlue": "#87CEEB",
      "terminal.ansiBrightBlue": "#ADD8E6",
      "terminal.ansiCyan": "#00CED1",
      "terminal.ansiBrightCyan": "#00FFFF",
      "terminal.ansiGreen": "#ff9d00",
      "terminal.ansiBrightGreen": "#ffb000",
      "terminal.ansiMagenta": "#DDA0DD",
      "terminal.ansiBrightMagenta": "#EE82EE",
      "terminal.ansiRed": "#FF0000",
      "terminal.ansiBrightRed": "#FF4444",
      "terminal.ansiWhite": "#FFE0D0",
      "terminal.ansiBrightWhite": "#FFFFFF",
      "terminal.ansiYellow": "#e99f17",
      "terminal.ansiBrightYellow": "#FFFF00",

       // Tab del terminal y paneles
       "terminal.tab.activeBorder": "#ff9d00",
       "panelSection.border": "#ff9d00",
       "panelSection.dropBackground": "#ff980040",
       
       
       // Barra de comandos del terminal
       "terminalCommandDecoration.defaultBackground": "#372413",
       "terminalCommandDecoration.successBackground": "#ff980040",
       "terminalCommandDecoration.errorBackground": "#FF000040",
       
    "editorHoverWidget.background": "#1F120A",
    "editorHoverWidget.border": "#ff9d00",
    "editorHoverWidget.foreground": "#ffb000",
    "editorHoverWidget.statusBarBackground": "#372413",


      // panel de comandos
    "commandCenter.background": "#1F120A",
    "commandCenter.border": "#ff98004a",
    //separator
    "commandCenter.activeBackground": "#422912",
    "commandCenter.foreground": "#ffb000",
    "commandCenter.activeForeground": "#ffb000",
    "commandCenter.activeBorder": "#ff7b00",
    "quickInput.background": "#1F120A",
    "quickInput.foreground": "#ffb000",
    "quickInputList.focusBackground": "#3F251A",
    "quickInputList.focusForeground": "#FFFFFF",
    "quickInputTitle.background": "#1F120A",
    "quickInputList.focusIconForeground": "#ffb000",
      
      // Input controls
      "input.background": "#1F120A",
      "input.foreground": "#ffb000",
      "input.border": "#ff9d00",
      "input.placeholderForeground": "#ffb00080",
      "inputOption.activeBackground": "#ff980040",
      "inputOption.activeBorder": "#ff9d00",
      "inputValidation.errorBackground": "#FF000020",
      "inputValidation.errorBorder": "#FF0000",
      "inputValidation.warningBackground": "#e99f1720",
      "inputValidation.warningBorder": "#e99f17",
      
      // Scrollbar
      "scrollbarSlider.background": "#ff980040",
      "scrollbarSlider.hoverBackground": "#ff980060",
      "scrollbarSlider.activeBackground": "#ff980080",
      "scrollbar.shadow": "#ff980020",
      
      // Breadcrumbs
      "breadcrumb.foreground": "#ffb00090",
      "breadcrumb.focusForeground": "#ffb000",
      "breadcrumb.activeSelectionForeground": "#FFFFFF",
      "breadcrumbPicker.background": "#1F120A",
      
      // Settings
      "settings.headerForeground": "#ffb000",
      "settings.modifiedItemIndicator": "#e99f17",
      "settings.checkboxBackground": "#1F120A",
      "settings.checkboxForeground": "#ffb000",
      "settings.checkboxBorder": "#ff9d00",
      
      // Debug
      "debugToolBar.background": "#1F120A",
      "debugToolBar.border": "#ff9d00",
      "debugIcon.breakpointForeground": "#FF0000",
      "debugIcon.startForeground": "#ffb000",
      
      // Git decorations
      "gitDecoration.modifiedResourceForeground": "#e99f17",
      "gitDecoration.deletedResourceForeground": "#FF0000",
      "gitDecoration.untrackedResourceForeground": "#ffb000",
      "gitDecoration.ignoredResourceForeground": "#ffb00040",
      "gitDecoration.conflictingResourceForeground": "#FFA500",
      "gitDecoration.submoduleResourceForeground": "#ffb00090",

      // Context menu
      "menu.background": "#1F120A",
      "menu.foreground": "#ffb000",
      "menu.selectionBackground": "#ff980040",
      "menu.selectionForeground": "#FFFFFF",
      "menu.separatorBackground": "#ff9d00bb",
      "menu.border": "#ff9d00bb",
