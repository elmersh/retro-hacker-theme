      "welcomePage.progress.foreground": "#ffb000",
      "walkThrough.embeddedEditorBackground": "#1F120A",

  },
  "tokenColors": [
    
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#8d7c6a90",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String and Templates",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated",
                "string.regexp",
                "string.tag",
                "string.value"
            ],
            "settings": {
                "foreground": "#fbebd5"
            }
        },
        {
          "scope": [
            "storage.modifier.async.js",
            "storage.type.js",
          ],
          "settings": {
            "foreground": "#ff9d00",
            "fontStyle": "bold"
          }
        },
        {
            "name": "JavaScript Template Expression",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#ffb000"
            }
        },
        {
            "name": "Operators and Delimiters",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#ffba14"
            }
        },
        {
            "name": "Tags and Attributes",
            "scope": [
                "entity.name.tag",
                "entity.other.attribute-name",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#ffb000"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#ffb000"
            }
        },
        {
            "name": "CSS Units",
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#e99f17"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color",
                "constant.other",
                "constant.character"
            ],
            "settings": {
                "foreground": "#e99f17"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars",
                "meta.function-call",
                "meta.method-call",
                "meta.function.parameters"
            ],
            "settings": {
                "foreground": "#ff7300",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof"
            ],
            "settings": {
                "foreground": "#ffc677",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types and Classes",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "storage.type.built-in",
                "meta.return-type",
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class",
                "entity.name.class",
                "source.cs storage.type"
            ],
            "settings": {
                "foreground": "#ffc46b",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "support.variable",
                "variable.other",
                "variable.language",
                "variable.parameter",
                "meta.definition.variable.name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#fcd498"
            }
        },
        {
            "name": "Language Variables",
            "scope": [
                "variable.language.this",
                "variable.language.super",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#ffb000",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Punctuation and Brackets",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.brackets",
                "meta.parentheses",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#8d5f02"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.css",
                "entity.other.attribute-name",
                "source.css entity.other.attribute-name",
                "source.css.less entity.other.attribute-name.id",
                "source.scss entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffb000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Properties and Values",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass",
                "support.type.vendored.property-name",
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media"
            ],
            "settings": {
                "foreground": "#fcd498"
            }
        },
        {
            "name": "Markdown",
            "scope": [
                "markup.heading",
                "markup.bold",
                "markup.italic",
                "markup.inserted",
                "markup.deleted",
                "markup.changed",
                "markup.underline"
            ],
            "settings": {
                "foreground": "#ffb000"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Git and Version Control",
            "scope": [
                "support.function.git-rebase",
                "constant.sha.git-rebase",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#ffb000"
            }
        },
        {
          "name": "",
          "scope": [
            "entity.name.type.namespace.cs",
          ],
          "settings": {
            "foreground": "#ffd36c",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "entity.other.attribute-name.html",
          ],
          "settings": {
            "foreground": "#fff727",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "punctuation.definition.entity.html",
          ],
          "settings": {
            "foreground": "#af7700",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "support.type.property-name.json",
          ],
          "settings": {
            "foreground": "#ffb700",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "punctuation.support.type.property-name",
          ],
          "settings": {
            "foreground": "#ffee00",
            "fontStyle": "bold"
          }
        }
    ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
      "namespace": "#ffd36c",
      "type": "#ffd36c",
      "class": "#ffd36c",
      "enum": "#ffd36c",
      "interface": "#ffd36c",
      "struct": "#ffd36c",
      "typeParameter": "#ffd36c",
      "parameter": "#fcd498",
      "variable": "#ffa931",
      "property": "#fcd498",
      "enumMember": "#e99f17",
      "event": "#ff7300",
      "function": "#ff7300",
      "method": "#ff7300",
      "macro": "#ffb000",
      "keyword": "#ffb000",
      "modifier": "#ffb000",
      "comment": "#c5a25a90",
      "string": "#ffbc7a",
      "number": "#e99f17",
      "regexp": "#ffbc7a",
      "operator": "#ffb000",
      "decorator": "#e99f17"
  }
}