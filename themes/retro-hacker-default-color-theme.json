      "property": "#98FB98",
      "enumMember": "#FFD700",
      "event": "#00FFFF",
      "function": "#00FFFF",
      "method": "#00FFFF",
      "macro": "#FF9500",
      "keyword": "#BF00FF",
      "modifier": "#BF00FF",
