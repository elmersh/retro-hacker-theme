        },
        {
            "name": "HTML Attributes",
            "scope": [
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#AEEE00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "HTML Entities",
            "scope": [
                "punctuation.definition.entity.html"
            ],
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Properties",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#2bff00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Quotes",
            "scope": [
                "string.quoted.double.json",
                "string.quoted.single.json"
            ],
            "settings": {
                "foreground": "#FFCC00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Punctuation",
            "scope": [
                "punctuation.support.type.property-name"
            ],
            "settings": {
                "foreground": "#00ff00",
                "fontStyle": "bold"
            }
