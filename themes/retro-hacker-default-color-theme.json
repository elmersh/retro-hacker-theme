          "storage.modifier.package.java",
          "storage.modifier.import.java"
        ],
        "settings": {
          "foreground": "#FF9500"
        } 
      }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
      "namespace": "#ff6aff",
      "type": "#ff6aff",
      "class": "#ff6aff",
      "enum": "#ff6aff",
      "interface": "#ff6aff",
      "struct": "#ff6aff",
      "typeParameter": "#ff6aff",
