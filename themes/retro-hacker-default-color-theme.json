        "function": "#AEEE00",
        "method": "#AEEE00",
        "macro": "#FFCC00",
        "keyword": "#66FF66",
        "modifier": "#66FF66",
        "comment": "#6B8E23",
        "string": "#FFCC00",
        "number": "#FFD700",
        "regexp": "#FFCC00",
