      // Activity bar
      "activityBar.background": "#1f0a1d",
      "activityBar.foreground": "#ff1a8d",
      "activityBar.border": "#410d32",
      "activityBar.activeBorder": "#ff1a8d",
      "activityBar.activeBackground": "#ff1a8d20",
      "activityBarBadge.background": "#ff1a8d",
      "activityBarBadge.foreground": "#000000",
      
      // Title and tabs
      "editorGroup.border": "#ff1a8d",
      "titleBar.border": "#ff1a8d",
