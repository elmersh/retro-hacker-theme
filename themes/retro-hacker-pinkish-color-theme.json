      "tab.activeForeground": "#ff1a8d",
      "tab.inactiveBackground": "#1f0a15",
      "tab.inactiveForeground": "#ff1a8d80",
      "tab.hoverBackground": "#ff1a8d20",
      
      // Status bar
      "statusBar.background": "#1f0a16",
      "statusBar.foreground": "#ff1a8d",
      "statusBar.border": "#ff1a8d",
      "statusBar.debuggingBackground": "#37132d",
      "statusBar.debuggingForeground": "#ff1a8d",
      "statusBar.debuggingBorder": "#ff1a8d",
      "statusBar.noFolderBackground": "#0A1F0A",
      "statusBar.noFolderForeground": "#ff1a8d",
      "statusBar.noFolderBorder": "#ff1a8d",
      "statusBarItem.hoverBackground": "#ff1a8d40",
      "statusBarItem.remoteBackground": "#ff1a8d",
      "statusBarItem.remoteForeground": "#000000",
      
      // Terminal panel
      "panel.background": "#1f0a1d",
      "panel.border": "#ff1a8d",
      "panelTitle.activeBorder": "#ff1a8d",
      "panelTitle.activeForeground": "#ff1a8d",
      "panelTitle.inactiveForeground": "#ff1a8d",
      "panelInput.border": "#ff1a8d",
      
      // Panel section headers
      "panelSectionHeader.background": "#371330",
      "panelSectionHeader.foreground": "#ff1a8d",
      "panelSectionHeader.border": "#ff1a8d",
      
      // Terminal
      "terminal.ansiBlack": "#000000",
      "terminal.ansiBrightBlack": "#1a1a1a",
      "terminal.ansiBlue": "#87CEEB",
      "terminal.ansiBrightBlue": "#ADD8E6",
      "terminal.ansiCyan": "#00CED1",
      "terminal.ansiBrightCyan": "#00FFFF",
      "terminal.ansiGreen": "#ff1a8d",
      "terminal.ansiBrightGreen": "#ff1a8d",
      "terminal.ansiMagenta": "#DDA0DD",
      "terminal.ansiBrightMagenta": "#EE82EE",
      "terminal.ansiRed": "#FF0000",
      "terminal.ansiBrightRed": "#FF4444",
      "terminal.ansiWhite": "#FFE0E0",
      "terminal.ansiBrightWhite": "#FFFFFF",
      "terminal.ansiYellow": "#FFD700",
      "terminal.ansiBrightYellow": "#FFFF00",

      // Terminal tab and panels
      "terminal.tab.activeBorder": "#ff1a8d",
      "panelSection.border": "#ff1a8d",
      "panelSection.dropBackground": "#ff1a8d40",
      
      // Integrated terminal tabs
      "tab.activeBorderTop": "#ff1a8d",
      "tab.unfocusedActiveBorderTop": "#ff1a8d80",
      
      // Terminal command bar
      "terminalCommandDecoration.defaultBackground": "#37132d",
      "terminalCommandDecoration.successBackground": "#ff1a8d40",
      "terminalCommandDecoration.errorBackground": "#FF000040",
      
      // Editor hover widget
      "editorHoverWidget.background": "#1f0a12",
      "editorHoverWidget.border": "#ff1a8d",
      "editorHoverWidget.foreground": "#ff1a8d",
      "editorHoverWidget.statusBarBackground": "#37132d",

      // Command center
      "commandCenter.background": "#1e0a1d",
      "commandCenter.border": "#ff1a8d41",
      "commandCenter.activeBackground": "#3f1a3a",
      "commandCenter.foreground": "#ff1a8d",
      "commandCenter.activeForeground": "#FFFFFF",
      "commandCenter.activeBorder": "#ff1a8d",
      "quickInput.background": "#1f0a15",
      "quickInput.foreground": "#ff1a8d",
      "quickInputList.focusBackground": "#3f1a3d",
      "quickInputList.focusForeground": "#FFFFFF",
      "quickInputTitle.background": "#1f0a1b",
      "quickInputList.focusIconForeground": "#ff1a8d",
      
      // Input controls
      "input.background": "#1f0a16",
      "input.foreground": "#ff1a8d",
      "input.border": "#58284080",
      "input.placeholderForeground": "#ff1a8d80",
      "inputOption.activeBackground": "#ff1a8d40",
      "inputOption.activeBorder": "#ff1a8d75",
      "inputValidation.errorBackground": "#FF000020",
      "inputValidation.errorBorder": "#FF0000",
      "inputValidation.warningBackground": "#6f00ff20",
      "inputValidation.warningBorder": "#FFD700",
      
      // Scrollbar
      "scrollbarSlider.background": "#ff1a8d40",
      "scrollbarSlider.hoverBackground": "#ff1a8d60",
      "scrollbarSlider.activeBackground": "#ff1a8d80",
      "scrollbar.shadow": "#ff1a8d20",
      
      // Breadcrumbs
      "breadcrumb.foreground": "#ff1a8d90",
      "breadcrumb.focusForeground": "#ff1a8d",
      "breadcrumb.activeSelectionForeground": "#FFFFFF",
      "breadcrumbPicker.background": "#1f0a18",
      
      // Settings
      "settings.headerForeground": "#ff1a8d",
      "settings.modifiedItemIndicator": "#FFD700",
      "settings.checkboxBackground": "#1f0a17",
      "settings.checkboxForeground": "#ff1a8d",
      "settings.checkboxBorder": "#ff1a8d",
      
      // Debug
      "debugToolBar.background": "#1f0a17",
      "debugToolBar.border": "#ff1a8d",
      "debugIcon.breakpointForeground": "#FF0000",
      "debugIcon.startForeground": "#ff1a8d",
      
      // Git decorations
      "gitDecoration.modifiedResourceForeground": "#FFD700",
      "gitDecoration.deletedResourceForeground": "#FF0000",
      "gitDecoration.untrackedResourceForeground": "#ff1a8d",
      "gitDecoration.ignoredResourceForeground": "#ff1a8d40",
      "gitDecoration.conflictingResourceForeground": "#FFA500",
      "gitDecoration.submoduleResourceForeground": "#ff1a8d90",

      // Context menu
      "menu.background": "#11040dc4",
      "menu.foreground": "#ff1a8d",
      "menu.selectionBackground": "#ff1a8d40",
      "menu.selectionForeground": "#FFFFFF",
      "menu.separatorBackground": "#ff1a8d",
      "menu.border": "#ff1a8d",
      "menu.selectionBorder": "#ff1a8d",

      // Notifications
      "notificationCenter.border": "#ff1a8d",
      "notificationCenterHeader.foreground": "#ff1a8d",
      "notificationCenterHeader.background": "#1f0a1a",
      "notificationToast.border": "#ff1a8d",
      "notifications.foreground": "#ff1a8d",
      "notifications.background": "#1f0a1a",
      "notifications.border": "#ff1a8d",
      "notificationLink.foreground": "#ff1a8d",

      // Extensions
      "extensionButton.prominentBackground": "#ff1a8d",
      "extensionButton.prominentHoverBackground": "#ff1a8d",
      "extensionIcon.starForeground": "#FFD700",

      // Welcome page
      "welcomePage.background": "#1f0a1a",
      "welcomePage.progress.background": "#ff1a8d",
      "welcomePage.progress.foreground": "#ff1a8d",
      "walkThrough.embeddedEditorBackground": "#1f0a1a"
  },
  "tokenColors": [
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#a08191",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String and Templates",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated",
                "string.regexp",
                "string.tag",
                "string.value"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "JavaScript Template Expression",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#ff50e8"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color",
                "constant.other",
                "constant.character"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars",
                "meta.function-call",
                "meta.method-call",
                "meta.function.parameters"
            ],
            "settings": {
                "foreground": "#c341ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof"
            ],
            "settings": {
                "foreground": "#fffd77",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types and Classes",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "storage.type.built-in",
                "meta.return-type",
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class",
                "entity.name.class",
                "source.cs storage.type"
            ],
            "settings": {
                "foreground": "#4EC9B0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "support.variable",
                "variable.other",
                "variable.language",
                "variable.parameter",
                "meta.definition.variable.name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#ec67b9"
            }
        },
        {
            "name": "Language Variables",
            "scope": [
                "variable.language.this",
                "variable.language.super",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#ff009d",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Punctuation and Brackets",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.brackets",
                "meta.parentheses",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#ff009d"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.css",
                "entity.other.attribute-name",
                "source.css entity.other.attribute-name",
                "source.css.less entity.other.attribute-name.id",
                "source.scss entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ff009d",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Properties and Values",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass",
                "support.type.vendored.property-name",
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Markdown",
            "scope": [
                "markup.heading",
                "markup.bold",
                "markup.italic",
                "markup.inserted",
                "markup.deleted",
                "markup.changed",
                "markup.underline"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#F44747",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Git and Version Control",
            "scope": [
                "support.function.git-rebase",
                "constant.sha.git-rebase",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        }
    ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
      "namespace": "#4EC9B0",
      "type": "#4EC9B0",
      "class": "#4EC9B0",
      "enum": "#4EC9B0",
      "interface": "#4EC9B0",
      "struct": "#4EC9B0",
      "typeParameter": "#4EC9B0",
      "parameter": "#9CDCFE",
      "variable": "#9CDCFE",
      "property": "#9CDCFE",
      "enumMember": "#B5CEA8",
      "event": "#DCDCAA",
      "function": "#DCDCAA",
      "method": "#DCDCAA",
      "macro": "#569CD6",
      "keyword": "#569CD6",
      "modifier": "#569CD6",
      "comment": "#6B8E23",
      "string": "#CE9178",
      "number": "#B5CEA8",
      "regexp": "#CE9178",
      "operator": "#D4D4D4",
      "decorator": "#B5CEA8"
  }
}