      "panelTitle.activeForeground": "#FF00FF",
      "panelTitle.inactiveForeground": "#920792",
      "panelInput.border": "#FF1493",
      
      // Panel section headers (Problems, Output, Debug Console, Terminal)
      "panelSectionHeader.background": "#371337",
      "panelSectionHeader.foreground": "#FF00FF",
