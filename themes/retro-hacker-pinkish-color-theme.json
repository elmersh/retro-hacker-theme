      // Terminal
      "terminal.background": "#000000",
      "terminal.foreground": "#FF00FF",
      "terminal.ansiBlack": "#000000",
      "terminal.ansiBrightBlack": "#1A1A1A",
      "terminal.ansiBlue": "#87CEEB",
      "terminal.ansiBrightBlue": "#ADD8E6",
      "terminal.ansiCyan": "#00CED1",
      "terminal.ansiBrightCyan": "#00FFFF",
      "terminal.ansiGreen": "#FF1493",
      "terminal.ansiBrightGreen": "#FF00FF",
      "terminal.ansiMagenta": "#DDA0DD",
      "terminal.ansiBrightMagenta": "#EE82EE",
      "terminal.ansiRed": "#FF4444",
      "terminal.ansiBrightRed": "#FF6666",
      "terminal.ansiWhite": "#FFE0FF",
      "terminal.ansiBrightWhite": "#FFFFFF",
      "terminal.ansiYellow": "#FF69B4",
      "terminal.ansiBrightYellow": "#FF99CC",
      
      // Tab del terminal y paneles
      "terminal.tab.activeBorder": "#FF1493",
      "panelSection.border": "#FF1493",
      "panelSection.dropBackground": "#FF00FF40",
      
      // Barra de comandos del terminal
      "terminalCommandDecoration.defaultBackground": "#371337",
      "terminalCommandDecoration.successBackground": "#FF00FF40",
      "terminalCommandDecoration.errorBackground": "#FF444440",
      
      "editorHoverWidget.background": "#1F0A1F",
      "editorHoverWidget.border": "#FF1493",
      "editorHoverWidget.foreground": "#FF00FF",
      "editorHoverWidget.statusBarBackground": "#371337",
      
      // Panel de comandos
      "commandCenter.background": "#1F0A1F",
      "commandCenter.border": "#FF144141",
      "commandCenter.activeBackground": "#2C002C",
      "commandCenter.foreground": "#FF00FF",
      "commandCenter.activeForeground": "#FF00FF",
      "commandCenter.activeBorder": "#8A008A",
      "quickInput.background": "#1F0A1F",
      "quickInput.foreground": "#FF00FF",
      "quickInputList.focusBackground": "#3F1A3F",
      "quickInputList.focusForeground": "#FFFFFF",
      "quickInputTitle.background": "#1F0A1F",
      "quickInputList.focusIconForeground": "#FF1493",
      
      // Input controls
      "input.background": "#1F0A1F",
      "input.foreground": "#FF00FF",
      "input.border": "#FF1493",
      "input.placeholderForeground": "#FF00FF80",
      "inputOption.activeBackground": "#FF00FF40",
      "inputOption.activeBorder": "#FF1493",
      "inputValidation.errorBackground": "#FF444420",
      "inputValidation.errorBorder": "#FF4444",
      "inputValidation.warningBackground": "#FF69B420",
      "inputValidation.warningBorder": "#FF69B4",
      
      // Scrollbar
      "scrollbarSlider.background": "#FF00FF40",
      "scrollbarSlider.hoverBackground": "#FF00FF60",
      "scrollbarSlider.activeBackground": "#FF00FF80",
      "scrollbar.shadow": "#FF00FF20",
      
      // Breadcrumbs
      "breadcrumb.foreground": "#FF00FF90",
      "breadcrumb.focusForeground": "#FF1493",
      "breadcrumb.activeSelectionForeground": "#FFFFFF",
      "breadcrumbPicker.background": "#1F0A1F",
      
      // Settings
      "settings.headerForeground": "#FF00FF",
      "settings.modifiedItemIndicator": "#FF69B4",
      "settings.checkboxBackground": "#1F0A1F",
      "settings.checkboxForeground": "#FF00FF",
      "settings.checkboxBorder": "#FF1493",
      
      // Debug
      "debugToolBar.background": "#1F0A1F",
      "debugToolBar.border": "#FF1493",
      "debugIcon.breakpointForeground": "#FF4444",
      "debugIcon.startForeground": "#FF00FF",
      
      // Git decorations
      "gitDecoration.modifiedResourceForeground": "#FF69B4",
      "gitDecoration.deletedResourceForeground": "#FF4444",
      "gitDecoration.untrackedResourceForeground": "#FF00FF",
      "gitDecoration.ignoredResourceForeground": "#FF00FF40",
      "gitDecoration.conflictingResourceForeground": "#FFA500",
      "gitDecoration.submoduleResourceForeground": "#FF00FF90",

      // Context menu
      "menu.background": "#1F0A1F",
      "menu.foreground": "#FF00FF",
      "menu.selectionBackground": "#FF00FF40",
      "menu.selectionForeground": "#FFFFFF",
      "menu.separatorBackground": "#FF1493BB",
      "menu.border": "#FF1493BB",
      "menu.selectionBorder": "#FF1493BB",

      // Notifications
      "notificationCenter.border": "#FF1493",
      "notificationCenterHeader.foreground": "#FF00FF",
      "notificationCenterHeader.background": "#1F0A1F",
      "notificationToast.border": "#FF1493",
      "notifications.foreground": "#FF00FF",
      "notifications.background": "#1F0A1F",
      "notifications.border": "#FF1493",
      "notificationLink.foreground": "#FF1493",

      // Extensions
      "extensionButton.prominentBackground": "#FF1493",
      "extensionButton.prominentHoverBackground": "#FF00FF",
      "extensionIcon.starForeground": "#FF69B4",

      // Welcome page
      "welcomePage.background": "#1F0A1F",
      "welcomePage.progress.background": "#FF1493",
      "welcomePage.progress.foreground": "#FF00FF",
      "walkThrough.embeddedEditorBackground": "#1F0A1F"
  },
  "tokenColors": [
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#8D799090",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String and Templates",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated",
                "string.regexp",
                "string.tag",
                "string.value"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
          "scope": [
            "storage.modifier.async.js",
            "storage.type.js"
          ],
          "settings": {
            "foreground": "#FF1493",
            "fontStyle": "bold"
          }
        },
        {
            "name": "JavaScript Template Expression",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end"
            ],
            "settings": {
                "foreground": "#FF00FF"
            }
        },
        {
            "name": "Operators and Delimiters",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#FF69B4"
            }
        },
        {
            "name": "Tags and Attributes",
            "scope": [
                "entity.name.tag",
                "entity.other.attribute-name",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#FF00FF"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#FF1493"
            }
        },
        {
            "name": "CSS Units",
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#FF69B4"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color",
                "constant.other",
                "constant.character"
            ],
            "settings": {
                "foreground": "#FF69B4"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars",
                "meta.function-call",
                "meta.method-call",
                "meta.function.parameters"
            ],
            "settings": {
                "foreground": "#87CEEB",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof"
            ],
            "settings": {
                "foreground": "#FF99CC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types and Classes",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "storage.type.built-in",
                "meta.return-type",
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class",
                "entity.name.class",
                "source.cs storage.type"
            ],
            "settings": {
                "foreground": "#DDA0DD",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "support.variable",
                "variable.other",
                "variable.language",
                "variable.parameter",
                "meta.definition.variable.name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#FFB6C1"
            }
        },
        {
            "name": "Language Variables",
            "scope": [
                "variable.language.this",
                "variable.language.super",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#FF00FF",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Punctuation and Brackets",
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.brackets",
                "meta.parentheses",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#8D2C8D"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.css",
                "entity.other.attribute-name",
                "source.css entity.other.attribute-name",
                "source.css.less entity.other.attribute-name.id",
                "source.scss entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#FF00FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Properties and Values",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass",
                "support.type.vendored.property-name",
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media"
            ],
            "settings": {
                "foreground": "#FFB6C1"
            }
        },
        {
            "name": "Markdown",
            "scope": [
                "markup.heading",
                "markup.bold",
                "markup.italic",
                "markup.inserted",
                "markup.deleted",
                "markup.changed",
                "markup.underline"
            ],
            "settings": {
                "foreground": "#FF1493"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#FF4444",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Git and Version Control",
            "scope": [
                "support.function.git-rebase",
                "constant.sha.git-rebase",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#FF00FF"
            }
        },
        {
          "name": "",
          "scope": [
            "entity.name.type.namespace.cs"
          ],
          "settings": {
            "foreground": "#FF69B4",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "entity.other.attribute-name.html"
          ],
          "settings": {
            "foreground": "#FF99CC",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "punctuation.definition.entity.html"
          ],
          "settings": {
            "foreground": "#FF1493",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "support.type.property-name.json"
          ],
          "settings": {
            "foreground": "#FF00FF",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "punctuation.support.type.property-name"
          ],
          "settings": {
            "foreground": "#FF69B4",
            "fontStyle": "bold"
          }
        }
    ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
      "namespace": "#FF69B4",
      "type": "#FF69B4",
      "class": "#FF69B4",
      "enum": "#FF69B4",
      "interface": "#FF69B4",
      "struct": "#FF69B4",
      "typeParameter": "#FF69B4",
      "parameter": "#FFB6C1",
      "variable": "#FF31FF",
      "property": "#FFB6C1",
      "enumMember": "#FF99CC",
      "event": "#87CEEB",
      "function": "#87CEEB",
      "method": "#87CEEB",
      "macro": "#FF00FF",
      "keyword": "#FF1493",
      "modifier": "#FF00FF",
      "comment": "#C25A9090",
      "string": "#FFA07A",
      "number": "#FF99CC",
      "regexp": "#FFA07A",
      "operator": "#FF1493",
      "decorator": "#FF69B4"
  }
}