  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#7C8DD990",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String and Templates",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated",
        "string.regexp",
        "string.tag",
        "string.value"
      ],
      "settings": {
        "foreground": "#BFD7FF"
      }
    },
    {
      "scope": ["storage.modifier.async.js", "storage.type.js"],
      "settings": {
        "foreground": "#5EAFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript Template Expression",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#5EAFFF"
      }
    },
    {
      "name": "Operators and Delimiters",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.definition"
      ],
      "settings": {
        "foreground": "#83B9FF"
      }
    },
    {
      "name": "Tags and Attributes",
      "scope": ["entity.name.tag", "entity.other.attribute-name", "meta.tag"],
      "settings": {
        "foreground": "#5EAFFF"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#5EAFFF"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Numbers and Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color",
        "constant.other",
        "constant.character"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "meta.function-call",
        "meta.method-call",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#87CEEB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords and Control",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof"
      ],
      "settings": {
        "foreground": "#A6CAFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Classes",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "storage.type.built-in",
        "meta.return-type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class",
        "entity.name.class",
        "source.cs storage.type"
      ],
      "settings": {
        "foreground": "#8A5EC0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables and Parameters",
      "scope": [
        "variable",
        "support.variable",
        "variable.other",
        "variable.language",
        "variable.parameter",
        "meta.definition.variable.name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#A6CAFF"
      }
    },
    {
      "name": "Language Variables",
      "scope": [
        "variable.language.this",
        "variable.language.super",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#5EAFFF",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Punctuation and Brackets",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.brackets",
        "meta.parentheses",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#6694CA"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.css",
        "entity.other.attribute-name",
        "source.css entity.other.attribute-name",
        "source.css.less entity.other.attribute-name.id",
        "source.scss entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#5EAFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Properties and Values",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass",
        "support.type.vendored.property-name",
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media"
      ],
      "settings": {
        "foreground": "#A6CAFF"
      }
    },
    {
      "name": "Markdown",
      "scope": [
        "markup.heading",
        "markup.bold",
        "markup.italic",
        "markup.inserted",
        "markup.deleted",
        "markup.changed",
        "markup.underline"
      ],
      "settings": {
        "foreground": "#5EAFFF"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF5A5A",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Git and Version Control",
      "scope": [
        "support.function.git-rebase",
        "constant.sha.git-rebase",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#5EAFFF"
      }
    },
    {
      "name": "",
      "scope": ["entity.name.type.namespace.cs"],
      "settings": {
        "foreground": "#B07CFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#A0C8FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["punctuation.definition.entity.html"],
      "settings": {
        "foreground": "#4A86DB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#5EAFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["punctuation.support.type.property-name"],
      "settings": {
        "foreground": "#4A86DB",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#B07CFF",
    "type": "#B07CFF",
    "class": "#B07CFF",
    "enum": "#B07CFF",
    "interface": "#B07CFF",
    "struct": "#B07CFF",
    "typeParameter": "#B07CFF",
    "parameter": "#A6CAFF",
    "variable": "#7CBEFF",
    "property": "#A6CAFF",
    "enumMember": "#FFD700",
    "event": "#87CEEB",
    "function": "#87CEEB",
    "method": "#87CEEB",
    "macro": "#5EAFFF",
    "keyword": "#5EAFFF",
    "modifier": "#5EAFFF",
    "comment": "#6A9FC590",
    "string": "#B0E0FF",
    "number": "#FFD700",
    "regexp": "#B0E0FF",
    "operator": "#5EAFFF",
    "decorator": "#FFD700"
  }
}
